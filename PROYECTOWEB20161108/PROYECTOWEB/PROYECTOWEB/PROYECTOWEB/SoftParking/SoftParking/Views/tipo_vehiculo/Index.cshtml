@model IEnumerable<SoftParking.Models.tipo_vehiculo>

@{
    ViewBag.Title = "Clases vehiculos";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("INSERTAR VEHICULOS", "Create")
</p>
<table class="table table-bordered " >
    <tr>
        <th>
           NOMBRE
        </th>
        <th>
           TARIFA
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nomb_vehi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tari_vehi)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.tipo_vehi }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.tipo_vehi })
        </td>
    </tr>
}

</table>
